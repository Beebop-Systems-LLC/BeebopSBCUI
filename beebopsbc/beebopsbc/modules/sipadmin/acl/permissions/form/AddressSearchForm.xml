        <Element Name="btn_cancel" Class="Button" Text="Cancel" CssClass="mdc-button mdc-button--raised">
            <EventHandler Name="cancel_onclick" Event="onclick" Function="SwitchForm(sipadmin.acl.permissions.form.AddressListForm)"  ShortcutKey="Escape" ContextMenu="Cancel"/>
        </Element>
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
