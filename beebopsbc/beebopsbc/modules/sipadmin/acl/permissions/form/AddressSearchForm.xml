        <Element Name="fld_grp" Class="InputText" FieldName="grp" Label="Grp"  AllowURLParam="N" CssClass="mdc-text-field__input" CssErrorClass="input_text_error" CssFocusClass="mdc-text-field__input"/>
        <Element Name="fld_ip_addr" Class="InputText" FieldName="ip_addr" Label="Ip Addr"  AllowURLParam="N" CssClass="mdc-text-field__input" CssErrorClass="input_text_error" CssFocusClass="mdc-text-field__input"/>
	<Element Name="fld_mask" Class="InputText" FieldName="mask" Label="Mask"  DefaultValue="32" AllowURLParam="N" CssClass="mdc-text-field__input" CssErrorClass="input_text_error" CssFocusClass="mdc-text-field__input"/>
        <Element Name="fld_port" Class="InputText" FieldName="port" Label="Port"  AllowURLParam="N" CssClass="mdc-text-field__input" CssErrorClass="input_text_error" CssFocusClass="mdc-text-field__input"/>
