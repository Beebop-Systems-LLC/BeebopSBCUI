<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="AddressSearchForm" Class="sipadmin.common.form.EasySearchForm" FormType="Search" jsClass="jbForm" Title="Search Address" Description="" BizDataObj="sipadmin.acl.permissions.do.AddressDO" TemplateEngine="Smarty" TemplateFile="detail.tpl" EventName="" MessageFile="">
