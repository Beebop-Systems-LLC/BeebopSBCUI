    <ActionPanel>
        <Element Name="btn_search" Class="Button" Text="Search" CssClass="mdc-button mdc-button--raised space-size">
			<EventHandler Name="search_onclick" EventLogMsg="" Event="onclick" Function="SwitchSearchForm(sipadmin.acl.permissions.form.AddressListForm)" RedirectPage=""  ShortcutKey="Ctrl+Enter" ContextMenu="Save" />
